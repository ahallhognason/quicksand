{
    "name": "tightenco/quicksand",
    "description": "Easily schedule regular cleanup of old soft-deleted Eloquent data.",
    "homepage": "https://github.com/tighten/quicksand",
    "license": "MIT",
    "authors": [
        {
            "name": "Benson Lee",
            "email": "benson@tighten.co"
        },
        {
            "name": "Matt Stauffer",
            "email": "matt@tighten.co"
        },
        {
            "name": "Jose Soto",
            "email": "jose@tighten.co"
        },
        {
            "name": "John Bonaccorsi",
            "email": "john@tighten.co"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/config": "~5.6|~5.7|~5.8|~6.0|~7.0|~8.0",
        "illuminate/console": "~5.6|~5.7|~5.8|~6.0|~7.0|~8.0",
        "illuminate/log": "~5.6|~5.7|~5.8|~6.0|~7.0|~8.0",
        "illuminate/support": "~5.6|~5.7|~5.8|~6.0|~7.0|~8.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.8",
        "laravel/legacy-factories": "^1.0.4",
        "mockery/mockery": "^1.0.0",
        "orchestra/testbench": "~3.0|~4.0|~5.0|~6.0",
        "phpunit/phpunit": "~8.0"
    },
    "autoload": {
        "psr-4": {
            "Tightenco\\Quicksand\\": "src"
        },
        "classmap": [
            "tests"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tightenco\\Quicksand\\QuicksandServiceProvider"
            ]
        }
    }
}
